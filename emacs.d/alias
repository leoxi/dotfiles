alias ll ls -lh $*
alias la ls -A $*
alias lla ll -A $*
alias ff find-file $1
alias emacs find-file $1
alias d dired $1
alias cd.. cd ..
alias cd... cd ../..
alias cd.... cd ../../..
alias cd..... cd ../../../..
alias cd/ cd /
alias grep grep $*
alias brews brew list -1
alias bubu brew update && brew upgrade && brew cleanup
